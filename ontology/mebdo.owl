    <!-- http://purl.obolibrary.org/obo/MEBDO_0000023 -->

    <owl:ObjectProperty rdf:about="&obo;MEBDO_0000023">
        <rdfs:label xml:lang="en">ZIP_mapping_to_FIPS</rdfs:label>
        <obo:IAO_0000115>THis relation is used for the mapping bettween a zip code zrea and a FIPS code area. Normally a FIPS code has 1:1 relation to a county. But Zip code dosen&apos;t. THe boundary of a ZIP code zone may located_in or overlaps with the boundary of a FIPS code zone. So here we use &apos;mapping&apos; to reflect this relation.</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">YL</obo:IAO_0000117>
        <owl:inverseOf rdf:resource="&obo;MEBDO_0000024"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/MEBDO_0000024 -->

    <owl:ObjectProperty rdf:about="&obo;MEBDO_0000024">
        <rdfs:label xml:lang="en">FIPS_mapping_to_ZIP</rdfs:label>
        <obo:IAO_0000117 xml:lang="en">YL</obo:IAO_0000117>
        <obo:IAO_0000115 xml:lang="en">see definition of &apos;ZIP_mapping_to_FIPS&apos;</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/MEBDO_0000025 -->

    <owl:ObjectProperty rdf:about="&obo;MEBDO_0000025">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">located_in_FIPS_area</rdfs:label>
        <obo:IAO_0000115 xml:lang="en">According to FIPS code&apos;s defintion, each state and county will assign one and only one FIPS code at a time. This relation can be express as: &apos;located in at all times&apos; only &apos;FIPS code area&apos; and &apos;located in at all time exactly 1 &apos;FIPS code area&apos;. We set this relation as functional, so it may capture the essence of this relation</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">YL</obo:IAO_0000117>
        <rdfs:subPropertyOf rdf:resource="&obo;BFO_0000082"/>
    </owl:ObjectProperty>
    


